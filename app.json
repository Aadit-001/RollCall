{
  "expo": {
    "name": "RollCall",
    "slug": "app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/rc3.png",
    "scheme": "rollcall",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "android": {
      "package": "com.pixelvolt.RollCall",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/rc3.png",
        "backgroundColor": "#121212"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pixelvolt.RollCall"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/rc3.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#121212"
        }
      ],
      [
        "@evennit/notifee-expo-plugin",
        {
          "androidAppExtension": true,
          "iosDeploymentTarget": "13.4",
          "apsEnvMode": "production",
          "backgroundModes": [
            "remote-notification"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f272a3a8-196e-4a8f-91fe-fa4224649cd2",
        "build": {
          "experimental": {
            "ios": {
              "appExtensions": [
                {
                  "targetName": "NotifeeNotificationServiceExtension",
                  "bundleIdentifier": "com.pixelvolt.RollCall.NotifeeNotificationServiceExtension",
                  "entitlements": {
                    "com.apple.security.application-groups": [
                      "group.com.pixelvolt.RollCall.notifee"
                    ]
                  }
                }
              ]
            },
            "android": {
              "appExtensions": [
                {
                  "targetName": "NotifeeNotificationServiceExtension",
                  "bundleIdentifier": "com.pixelvolt.RollCall.NotifeeNotificationServiceExtension",
                  "entitlements": {
                    "com.apple.security.application-groups": [
                      "group.com.pixelvolt.RollCall.notifee"
                    ]
                  }
                }
              ]
            }
          }
        }
      }
    },
    "owner": "pixy001"
  }
}
