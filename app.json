{
  "expo": {
    "name": "RollCall",
    "slug": "roll-call",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/rc3.jpeg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/rc3.jpeg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.RollCall"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/rc3.jpeg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/rc3.jpeg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "@evennit/notifee-expo-plugin",
        {
          "androidAppExtension": true,
          "iosDeploymentTarget": "13.4",
          "apsEnvMode": "production",
          "backgroundModes": [
            "remote-notification"
          ]
        }

      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "97bbd1ff-583a-4660-90a8-6d2ab14067ab",
        "build": {
          "experimental": {
            "ios": {
              "appExtensions": [
                {
                  "targetName": "NotifeeNotificationServiceExtension",
                  "bundleIdentifier": "com.anonymous.RollCall.NotifeeNotificationServiceExtension",
                  "entitlements": {
                    "com.apple.security.application-groups": [
                      "group.com.anonymous.RollCall.notifee"
                    ]
                  }
                }
              ]
            },
            "android": {
              "appExtensions": [
                {
                  "targetName": "NotifeeNotificationServiceExtension",
                  "bundleIdentifier": "com.anonymous.RollCall.NotifeeNotificationServiceExtension",
                  "entitlements": {
                    "com.apple.security.application-groups": [
                      "group.com.anonymous.RollCall.notifee"
                    ]
                  }
                }
              ]
            }
          }
        }
      }
    },
    "owner": "harsh002"
  }
}
